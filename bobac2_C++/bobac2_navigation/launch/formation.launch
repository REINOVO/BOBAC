<!--                                                                            
  bobac2 navigation simulation:
  - gazebo
  - map_server
  - move_base
  - amcl
  - rviz view
 --> 

<launch>
	<!--gazebo-->
	<!--include file="$(find bobac2_description)/launch/gazebo.launch"/-->
	<include file="$(find bobac2_description)/launch/8.launch"/>
	<!--map_server-->
	<include file="$(find bobac2_navigation)/launch/map_server.launch"/>
	<!--move_base-->
	<!--include file="$(find bobac2_navigation)/launch/move_base.launch"/-->
	<!--amcl-->
	<include file="$(find bobac2_navigation)/launch/amcl.launch"/>
	<include file="$(find bobac2_navigation)/launch/multi_amcl1.launch"/>
	<include file="$(find bobac2_navigation)/launch/multi_amcl2.launch"/>
	<!--  **************** Visualisation ****************  -->
  	<node name="rviz" pkg="rviz" type="rviz" args="-d $(find bobac2_navigation)/rviz/formation.rviz"/>

</launch>
